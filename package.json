{
  "name": "tamia",
  "description": "Tiny but extremely opinionated frontend framework",
  "version": "5.0.20",
  "sideEffects": false,
  "scripts": {
    "start": "npm run styleguide",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "prepublishOnly": "npm run compile",
    "lint": "eslint . --cache --fix --ext .tsx",
    "typecheck": "tsc --noEmit",
    "compile": "npm run compile:babel:cjs && npm run compile:babel:esm && npm run compile:types",
    "compile:babel:cjs": "babel --delete-dir-on-start --extensions '.tsx' -d lib/ src/",
    "compile:babel:esm": "BABEL_ENV=esm babel --delete-dir-on-start --extensions '.tsx' -d dist/ src/",
    "compile:types": "tsc --emitDeclarationOnly",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d styleguide/public",
    "docs": "npm run styleguide:build && npm run deploy",
    "format": "prettier --write '**/*.{tsx,md}'"
  },
  "homepage": "https://tamiadev.github.io/tamia/",
  "author": {
    "name": "Artem Sapegin",
    "url": "https://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamiadev/tamia.git"
  },
  "bugs": {
    "url": "https://github.com/tamiadev/tamia/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "browser": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@types/styled-system": "^5.1.7",
    "csstype": "^3.0.8",
    "styled-system": "^5.1.5",
    "system-props": "^0.20.0"
  },
  "peerDependencies": {
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/styled-components": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^7.30.0",
    "eslint-config-tamia": "^7.2.6",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-docgen-typescript": "^2.0.0",
    "react-dom": "^16.14.0",
    "react-styleguidist": "12.0.0-alpha9.9",
    "styled-components": "^5.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": "eslint --cache --fix",
    "*.{tsx,md}": "prettier --write"
  }
}
